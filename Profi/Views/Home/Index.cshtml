<section class=section id=testimonials>
    <div class="section-content container">
        <div class="d-flex justify-content-center justify-content-sm-between align-items-center">
            <div class="text-center text-sm-left">
                <small class="fs-1 font-family-secondary text-uppercase font-weight-bold letter-spacing-caption text-muted">
                    #1
                    Rated Beauty service in NY
                </small>
                <h1>Testimonials</h1>
            </div>
            <div class="d-none d-sm-block">
                <div class=btn-group>
                    <button type=button class="btn btn-light btn-square" data-toggle=card-slider
                            data-target=#testimonials-slider data-action=prevSlide>
                        <i class=material-icons>arrow_left</i>
                    </button> <button type=button
                                      class="btn btn-light btn-square" data-toggle=card-slider data-target=#testimonials-slider
                                      data-action=nextSlide>
                        <i class=material-icons>arrow_right</i>
                    </button>
                </div>
            </div>
        </div>
        <div class="card-slider card-slider-viewport card-slider-services mt-15" id=testimonials-slider>
            <!--Card Item-->
            @foreach (var testimonial in Model.Testimonials)
            {
                <article class=card-slider-item>
                    <div class="card border-light card-accent-left" data-mh=testimonials-slider>
                        <div class="card-body p-19 p-md-20 d-flex flex-column">
                            <div class="d-flex align-items-center">
                                <img src="~/assets/images/@testimonial.Url" alt=avatar
                                 width=50 class=rounded-circle>

                                <div class=ml-9>
                                    <div class="text-black font-weight-medium fs-5 lh-5 mb-4">@testimonial.Name</div>
                                    <div class="text-muted fs-3 lh-4">@testimonial.Speciality</div>
                                </div>
                            </div>
                            <blockquote class=my-14>
                                <p>
                                    @testimonial.Description
                                </p>
                            </blockquote>
                            <div class=star-rating>
                                @if((double)testimonial.Rating % (double)2 == 0)
                                {
                                    for(int i=0; i < testimonial.Rating / 2; i++)
                                    {
                                        <i class=material-icons>star</i>
                                    }
                                    for(int i=0;i < 5-testimonial.Rating/2; i++)
                                    {
                                        <i class=material-icons>star_border</i>
                                    }
                                }
                                @if((double)testimonial.Rating % (double)2 != 0)
                                {
                                    for(int i=0; i < (testimonial.Rating / 2)- 0.5; i++)
                                    {
                                        <i class=material-icons>star</i>
                                    }
                                    <i class=material-icons>star_half</i>
                                    for(int i=1;i < 5-(((testimonial.Rating/2)-0.5)+1); i++)
                                    {
                                        <i class=material-icons>star_border</i>
                                    }
                                }
                            </div>
                        </div>
                    </div>
                </article>
            }
        </div>
    </div>
</section>

